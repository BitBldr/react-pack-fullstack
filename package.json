  "keywords": [],
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.5.8",
    "babel-loader": "^5.1.4",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.9.3",
    "css-loader": "^0.14.5",
    "file-loader": "^0.8.4",
    "harmony": "0.0.1",
    "jade": "^1.11.0",
    "jade-loader": "^0.7.1",
    "json-loader": "^0.5.2",
    "jsx-loader": "^0.13.2",
    "node-sass": "^3.3.3",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^2.0.1",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.9.0"
  },
  "dependencies": {
    "alt": "^0.17.3",
    "bootstrap": "^3.3.5",
    "ejs": "~0.8.4",
    "grunt": "0.4.2",
    "grunt-contrib-clean": "~0.5.0",
    "grunt-contrib-coffee": "~0.10.1",
    "grunt-contrib-concat": "~0.3.0",
    "grunt-contrib-copy": "~0.5.0",
    "grunt-contrib-cssmin": "~0.9.0",
    "grunt-contrib-jst": "~0.6.0",
    "grunt-contrib-less": "0.11.1",
    "grunt-contrib-uglify": "~0.4.0",
    "grunt-contrib-watch": "~0.5.3",
    "grunt-sails-linker": "~0.9.5",
    "grunt-sync": "~0.0.4",
    "include-all": "~0.1.3",
    "jquery": "^2.1.4",
    "rc": "~0.5.0",
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "sails": "~0.11.0",
    "sails-disk": "~0.10.0"
  },
  "scripts": {
    "env": "env",
    "watch": "cp ./client/index.html ./index.html && webpack-dev-server --progress --colors --hot",
    "build": "cp ./client/index.html ./index.html && webpack",
    "clean": "rm ./index.html && rm -rf ./assets/app",
    "debug": "cp ./client/index.html ./index.html && webpack && node debug app.js",
    "start": "cp ./client/index.html ./index.html && webpack && node app.js"
  },
